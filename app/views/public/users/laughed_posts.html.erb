<h1>笑った一覧</h1>
<% @laughed_posts.each do |post| %>
    <p>画像</p>
    <%= link_to(post_path(post)) do %>
        <%= image_tag post.get_image(200,200) %>
    <% end %>
    <p>タイトル</p>
    <%= post.posted_title %>
    <p>本文</p>
    <%= post.post_content %>
<% end %>
<div>
    <%= paginate @laughed_posts %>
</div>